{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "patterns": [],
  "settings": {
		"color": {
			"background": false,
			"custom": false,
			"customGradient": false,
			"gradients": false,
			"palette": false
		},
		"spacing": {
			"margin": false,
			"padding": false,
			"blockGap": false,
			"units": [ "px", "rem", "vh", "vw" ]
		},
		"typography": {
			"customFontSize": true,
			"lineHeight": false,
			"dropCap": false,
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"textDecoration": false,
			"textTransform": false,
			"fontSizes": [
				{
					"name": "Normal",
					"size": "var(--type__size)",
					"slug": "normal"
				},
				{
					"name": "Large",
					"size": "calc(var(--type__size)*2)",
					"slug": "large"
				}
			],
			"fontFamilies": []
		},
    "blocks": {
      "core/button": {
        "border": {
          "radius": false
        },
        "color": {
        	"background": false,
        	"custom": false,
        	"customGradient": false,
        	"gradients": false,
          "palette": false
        },
				"typography": {
					"customFontSize": false,
					"fontSizes": []
				},
        "width": {
        	"custom": false
        }
      },
      "core/heading": {
        "color": {
        	"background": false,
        	"custom": false,
        	"customGradient": false,
        	"gradients": false,
          "palette": false
        },
				"typography": {
					"customFontSize": false,
					"fontSizes": []
				}
      },
      "core/pullquote": {
        "border": {
          "radius": false,
          "color": false
        },
				"typography": {
					"customFontSize": false,
					"fontSizes": []
				}
      },
      "core/quote": {
				"typography": {
					"customFontSize": false,
					"fontSizes": []
				}
      },
      "core/table": {
        "color": {
        	"background": false,
        	"custom": false,
        	"customGradient": false,
        	"gradients": false,
          "palette": false
        },
				"typography": {
					"customFontSize": false,
					"fontSizes": []
				}
      },
    }
  },
	"styles": {
		"spacing": {
			"blockGap": "calc(var(--type__size) * 2)"
		}
	}
}